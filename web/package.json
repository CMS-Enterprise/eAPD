    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "rootDir": "src",
    "setupFiles": [
      "../polyfills.test.js",
      "../setup.test.js"
    ],
    "snapshotSerializers": [
