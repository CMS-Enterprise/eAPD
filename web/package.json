{
  "name": "cms-hitech-apd-webapp",
  "version": "1.0.0",
  "description": "APD app for CMS HITECH program",
  "main": "app.js",
  "scripts": {
    "audit": "npx audit-ci --config ./audit-ci.json",
    "make-state-icons": "babel-node scripts/stateIcons.js && cp src/data/states-small/* src/static/img/states/",
    "scaffold": "rm -rf dist && mkdir dist && cp -r src/static dist/",
    "build": "npm run scaffold && NODE_ENV=production webpack -p",
    "lint": "eslint 'src/**/*.js'",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "test:generate-output": "TZ=utc jest --json --outputFile=.jest-test-results.json",
    "test": "TZ=utc jest --collectCoverage",
    "test-watch": "TZ=utc npm run test:generate-output -- --watch",
    "prettier": "prettier --config \"../.prettierrc\" --write \"src/**/*.js\"",
    "prebuild:storybook": "npm run scaffold && npm run test:generate-output",
    "build-storybook": "npm run prebuild:storybook && build-storybook -o ../docs-build -c .storybook -s ./dist",
    "storybook": "npm run scaffold && start-storybook -s ./dist -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/eAPD.git"
