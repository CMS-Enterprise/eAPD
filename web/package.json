{
  "name": "cms-hitech-apd-webapp",
  "version": "1.0.0",
  "description": "APD app for CMS HITECH program",
  "main": "app.js",
  "scripts": {
    "scaffold":
      "mkdir -p dist && cp src/index.html dist/ && cp ../Staticfile dist/",
    "build": "npm run scaffold && NODE_ENV=production webpack -p",
    "lint": "eslint 'src/**/*.js'",
    "start":
      "webpack-dev-server --host 0.0.0.0 --port 8001 --history-api-fallback --content-base src/",
    "test": "jest --collectCoverage",
    "test-watch": "jest --watch",
    "prettier":
      "prettier --single-quote --trailing-comma none --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/cms-hitech-apd.git"
  },
  "keywords": ["cms", "medicaid", "hitech", "emr", "apd"],
  "contributors": [
    "Nicole Fenton",
    "Meghana Khandekar",
    "Nikki Lee",
    "Brendan Sudol",
    "Greg Walker"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/cms-hitech-apd/issues"
  },
  "homepage": "https://github.com/18F/cms-hitech-apd#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "lodash.kebabcase": "^4.1.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-x-ray": "^1.0.0-4",
    "rebass": "^1.0.4",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.14.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^21.2.1",
    "jest-styled-components": "^4.10.0",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "babel": {
    "presets": ["env", "react", "stage-2"]
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "rootDir": "src",
    "setupFiles": [
      "../polyfills.test.js",
      "../setup.test.js",
      "../files.test.js"
    ]
  }
}
