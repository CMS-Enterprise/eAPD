    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@tinymce/tinymce-react": "^3.3.2",
    "axios": "^0.19.0",
    "connected-react-router": "^6.6.1",
    "d3-format": "^1.4.3",
    "detect-browser": "^4.8.0",
    "history": "^4.10.1",
    "i18n-js": "^3.5.1",
    "jsonpatch": "^3.0.1",
    "markdown-it": "^10.0.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
