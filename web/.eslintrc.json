{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [1, "never"],
    "react/jsx-filename-extension": 0
  },
  "env": {
    "browser": true
  },
  "overrides": [{
    "files": [ "**/*.test.js" ],
    "globals": {
      "describe": false,
      "expect": false,
      "test": false
    }
  }]
}
