{
  "name": "cms-hitech-apd-api",
  "version": "1.0.0",
  "description": "APD api for CMS HITECH program",
  "main": "main.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "make-migrate": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "start": "node main.js",
    "start-dev": "nodemon ./main.js -e js",
    "jest": "NODE_ENV=test jest",
    "test": "NODE_ENV=test tap -J --cov --coverage-report=lcov --no-browser --no-timeout --reporter=spec --test-arg=--silent '{,!(node_modules)/**/}*.test.js'",
    "test-endpoints": "jest --runInBand '.+\\.endpoint\\.js'",
    "test-specific": "NODE_ENV=test tap --cov --coverage-report=lcov --no-browser --reporter=spec",
    "prettier": "prettier --single-quote --trailing-comma none --write \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/cms-hitech-apd.git"
  },
  "keywords": [
    "cms",
    "medicaid",
    "hitech",
    "emr",
    "apd"
  ],
  "contributors": [
    "Nicolas Aretakis",
    "Heather Battaglia",
    "Ronald Bronson",
    "Nicole Fenton",
    "Eghosa Guobadia",
    "Austin Hernandez",
    "James Hupp",
    "Meghana Khandekar",
    "Jerome Lee",
    "Nikki Lee",
    "Christopher (CM) Lubinksi",
    "Aviva Oskow",
    "Laura Ponc√©",
    "Brendan Sudol",
    "Jen Thibault",
    "Greg Walker"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/cms-hitech-apd/issues"
  },
  "homepage": "https://github.com/18F/cms-hitech-apd#readme",
  "dependencies": {
    "ajv": "^6.10.2",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.12.1",
    "compression": "^1.7.3",
    "cookies": "^0.7.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fs-blob-store": "^5.2.1",
    "jsonpatch": "^3.0.1",
    "jsonpointer": "^4.0.1",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.14.6",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "uuid": "^3.3.3",
    "winston": "^2.4.4",
    "zxcvbn": "^4.4.2"
  },
  "engines": {
    "node": "^10.14"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "sinon": "^7.4.2",
    "stream-mock": "^2.0.5",
    "tap": "^14.6.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test|endpoint).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "utils.endpoint.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
